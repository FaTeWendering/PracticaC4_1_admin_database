             <widget class="QStackedWidget" name="stackedWidget">
              <property name="styleSheet">
               <string notr="true">/* --- ESTILO BASE PARA TODAS LAS PÁGINAS --- */
QStackedWidget &gt; QWidget {
    background-color: rgb(53, 53, 79); /* Fondo principal (azul oscuro) */
    font-family: &quot;Tahoma&quot;, sans-serif; /* Fuente base limpia */
}

/* --- TÍTULOS (Ej: &quot;Control de compras&quot;) --- */
/* Debes asignar esta propiedad en Qt Designer: cssClass = titulo */
QLabel[cssClass=&quot;titulo&quot;] {
    font-size: 16pt;
    font-weight: bold;
    color: rgb(20, 200, 220); /* Color Cián principal */
    background-color: transparent; /* Sin fondo */
}

/* --- ETIQUETAS NORMALES (Ej: &quot;User:&quot;, &quot;Nombre del proveedor:&quot;) --- */
QLabel {
    font-size: 12pt;
    color: white; /* Blanco para mejor contraste */
    background-color: transparent; /* Sin fondo */
}

/* --- CAMPOS DE ENTRADA (Texto, Listas, Fechas) --- */
QLineEdit, QComboBox, QDateEdit {
    font-size: 10pt;
    color: white;
    background-color: #000000ff; /* Fondo negro */
    border: 1px solid rgb(20, 200, 220); /* Borde Cián */
    border-radius: 5px;
    padding: 4px;
}

QComboBox::drop-down {
    border: none; /* Quita la flecha fea del combobox */
}

/* --- TABLA --- */
QTableWidget {
    font-size: 10pt;
    color: white;
    background-color: #000000ff; /* Fondo negro */
    gridline-color: rgb(53, 53, 79); /* Líneas de la cuadrícula */
    border: 1px solid rgb(20, 200, 220); /* Borde Cián */
}

/* --- ENCABEZADO DE LA TABLA (Cant, Descripción...) --- */
QHeaderView::section {
    background-color: rgb(20, 200, 220); /* Fondo Cián */
    color: black; /* Texto negro para contraste */
    font-weight: bold;
    padding: 4px;
    border: 1px solid rgb(53, 53, 79);
}</string>
